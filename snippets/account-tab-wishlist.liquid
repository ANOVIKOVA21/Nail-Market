<script src="{{ 'quick-add.js' | asset_url }}" defer="defer"></script>
{% comment %} {% assign wishlist = empty %} {% endcomment %}
{% comment %} {% assign wishlist = 5 %} {% endcomment %}
<section id="wishlist" class="tab-content">
  <div class="tab-content-container">
    {% comment %}
      {%- if wishlist != empty -%}
        <h1 class="customer__title">{{ 'customer.wishlist.title' | t }}</h1>
        <ul
          class="grid product-grid contains-card contains-card--product contains-card--standard grid--3-col-desktop grid--2-col-tablet grid--2-col-tablet-down"
          role="list"
        >
          {%- for product in collections.sale.products limit: 8 -%}
            <li class="grid__item">
              <div class="card-wrapper product-card-wrapper">
                <div class="card card--standard card--media">
                  <div class="card__inner gradient ratio" style="--ratio-percent:100%;">
                    <div class="card__media">
                      <div class="media media--transparent media--hover-effect">
                        <img
                          src="PRODUCT_IMAGE_URL"
                          alt="PRODUCT_TITLE"
                          class="motion-reduce"
                          loading="lazy"
                          width="300"
                          height="300"
                        >
                        <img
                          src="PRODUCT_SECONDARY_IMAGE_URL"
                          alt="PRODUCT_TITLE"
                          class="motion-reduce"
                          loading="lazy"
                          width="300"
                          height="300"
                          style="position:absolute;left:0;top:0;opacity:0;transition:opacity .3s;"
                        >
                      </div>
                    </div>
                    <div class="card__content">
                      <div class="card__utility">
                        <button class="card__wishlist-button">
                          <span class="svg-wrapper">
                            <svg
                              width="24"
                              height="24"
                              viewBox="0 0 24 24"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <path d="M4.222 13.5L12 21L19.778 13.5C21.128 12.4835 22 10.872 22 9.0555C22 5.989 19.511 3.5 16.4445 3.5C14.628 3.5 13.011 4.378 12 5.728C10.989 4.378 9.372 3.5 7.5555 3.5C4.489 3.5 2 5.989 2 9.0555C2 10.872 2.872 12.4835 4.222 13.5ZM7.5555 4.5C8.9805 4.5 10.3425 5.183 11.1995 6.327L12 7.396L12.8005 6.3275C13.6575 5.183 15.0195 4.5 16.4445 4.5C18.9565 4.5 21 6.5435 21 9.0555C21 10.4995 20.3355 11.8285 19.176 12.701L19.1275 12.7375L19.0835 12.78L12 19.611L4.9165 12.78L4.8725 12.7375L4.824 12.701C3.665 11.8285 3 10.4995 3 9.0555C3 6.5435 5.0435 4.5 7.5555 4.5Z" fill="#444444"></path>
                            </svg>
                          </span>
                        </button>
                      </div>
                      <div class="card__information">
                        <h3 class="card__heading">
                          <a
                            href="PRODUCT_URL"
                            class="full-unstyled-link"
                          >
                            PRODUCT_TITLE
                          </a>
                        </h3>
                      </div>
                    </div>
                    <div class="card__content">
                      <div class="card__information">
                        <h3 class="card__heading h5">
                          <a href="PRODUCT_URL" class="full-unstyled-link"> PRODUCT_TITLE </a>
                        </h3>
                        <div class="card-information">
                          <div class="card__reviews">
                            <div class="card__rating">
                              <span class="svg-wrapper">★</span>
                              <span class="svg-wrapper">★</span>
                              <span class="svg-wrapper">★</span>
                              <span class="svg-wrapper">★</span>
                              <span class="svg-wrapper">★</span>
                            </div>
                            <p class="card__review-count">100 reviews</p>
                          </div>
                          <span class="caption-large light"></span>
                          <div class="price">
                            <span class="price-item price-item--regular">PRODUCT_PRICE</span>
                          </div>
                        </div>
                      </div>
                      <div class="card__buttons">
                        <a href="PRODUCT_URL" class="card__details-btn button">Details</a>
                        <div class="quick-add no-js-hidden">
                          <product-form>
                            <form
                              method="post"
                              action="/cart/add"
                              accept-charset="UTF-8"
                              class="form"
                              enctype="multipart/form-data"
                              novalidate="novalidate"
                              data-type="add-to-cart-form"
                            >
                              <input type="hidden" name="form_type" value="product"
                              ><input type="hidden" name="utf8" value="✓"
                              ><input type="hidden" name="id" value="46740074070171" class="product-variant-id">
                              <button
                                id="quick-add-8539881406619-submit"
                                type="submit"
                                name="add"
                                class="quick-add__submit button button--full-width button--secondary"
                                aria-haspopup="dialog"
                                aria-labelledby="quick-add-8539881406619-submit title--8539881406619"
                                aria-live="polite"
                                data-sold-out-message="true"
                              >
                                <span>Buy now </span>
                                <span class="sold-out-message hidden"> Sold out </span>

                                <div class="loading__spinner hidden">
                                  <svg xmlns="http://www.w3.org/2000/svg" class="spinner" viewBox="0 0 66 66">
                                    <circle stroke-width="6" cx="33" cy="33" r="30" fill="none" class="path"></circle>
                                  </svg>
                                </div></button
                              ><input type="hidden" name="product-id" value="8539881406619"
                              ><input type="hidden" name="section-id" value="template--18865795825819__main">
                            </form>
                          </product-form>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </li>
          {%- endfor -%}
        </ul>
      {% else %}
        <div class="wishlist__warnings">
          <h1 class="wishlist__empty-text">{{ 'customer.wishlist.empty' | t }}</h1>
          <a
            href="{{ routes.all_products_collection_url }}"
            class="wishlist__button button-dark button"
            style="margin-bottom: 5rem;"
          >
            {{ 'general.go_shopping' | t }}
          </a>
        </div>
      {% endif %}
    {% endcomment %}
  </div>
</section>
